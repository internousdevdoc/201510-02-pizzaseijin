<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<constant name="struts.devMode" value="true" />
	<package name="jp.co.internous.action" extends="struts-default" namespace="/">

      <action name="GoTop"
         class="com.internousdev.breadPrototype.action.GoTopAction"
         method="execute">
         <result name="success">top.jsp</result>
         <result name="error">top.jsp</result>
        </action>

<action name="GoRedBread"
         class="com.internousdev.breadPrototype.action.GoRedBreadTestAction"
         method="execute">
         <result name="success">redBread.jsp</result>
         <result name="error">top.jsp</result>
        </action>
        
        <action name="GoBlueBread"
         class="com.internousdev.breadPrototype.action.GoBlueBreadTestAction"
         method="execute">
         <result name="success">blueBread.jsp</result>
         <result name="error">top.jsp</result>
        </action>
        
        <action name="GoGreenBread"
         class="com.internousdev.breadPrototype.action.GoGreenBreadTestAction"
         method="execute">
         <result name="success">greenBread.jsp</result>
         <result name="error">top.jsp</result>
        </action>


</package>
</struts>